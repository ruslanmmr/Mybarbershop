.service-filter
  .service-filter__background.background.lazy(data-src='img/backgrounds/bg-logos.png')
  .service-filter__container
    form(action="").service-filter-form
      .container
        .service-filter-form__section.service-filter-form__section_search
          .search
            input(type="text" placeholder="Город / метро Арбатская / улица Арбат / Выбрать сеть / мастер и тд. ..." required).input.input_style1.search__input
            button.search__button
              i.fas.fa-search
          span.service-filter-form__search-count всего найдено: 7 мест
        .service-filter-form__section.service-filter-form__section_filter
          .service-filter-form__container
            .service-filter-form__title Режим работы:
            .service-filter-form__content
              .service-filter-value
                label.label.label_style1.label_value1
                  .service-filter-value__title с утра
                  input(type="checkbox")
              .service-filter-value
                label.label.label_style1.label_value2
                  .service-filter-value__title до поздна
                  input(type="checkbox")
              .service-filter-value
                label.label.label_style1.label_value3
                  .service-filter-value__title круглосуточно
                  input(type="checkbox")
          span.service-filter-form__decoration
          .service-filter-form__container
            .service-filter-form__title Стоимость услуг:
            .service-filter-form__content
              .service-filter-value
                label(title='от 100 до 550р').label.label_style2.label_value4.tooltip
                  .service-filter-value__title Premium
                  input(type="checkbox")
              .service-filter-value
                label(title='от 550 до 1000р').label.label_style2.label_value5.tooltip
                  .service-filter-value__title Standard
                  input(type="checkbox")
              .service-filter-value
                label(title='от 1000р').label.label_style2.label_value6.tooltip
                  .service-filter-value__title Economy
                  input(type="checkbox")
    .service-filter-recomendations
      .decotarive-line2
        span
        i.fas.fa-thumbs-up
        span
      .container
        ul.service-filter-recomendations__list
          li.service-filter-recomendations__item
            a(href="#").service-filter-recomendations__link <strong>Бритва</strong> /на Новоясеневском проспекте/
          li.service-filter-recomendations__item
            a(href="#").service-filter-recomendations__link <strong>TopGun</strong> /в ТЦ МЕГА Химки/
          li.service-filter-recomendations__item
            a(href="#").service-filter-recomendations__link <strong>Barbarossa</strong> /на метро Ясенево/
  include map
