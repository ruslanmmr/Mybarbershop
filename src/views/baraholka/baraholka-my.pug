doctype html
html(lang="ru")
  head
    include ../blocks/head
  body
    .background.lazy(data-src='img/backgrounds/background.jpg')
    .wrapper
      .page
        include ../blocks/header
        .page__container.container
          aside.aside
            .account-nav
              ul.account-nav__list
                li.account-nav__item
                  a(href="baraholka/baraholka-all.html").account-nav__link Все обьявления
                li.account-nav__item
                  a(href="baraholka/baraholka.html").account-nav__link Дать объявление
                li.account-nav__item
                  a(href="baraholka/baraholka-my.html").account-nav__link.active Мои объявления
          main.main
            section.section
              .section__head
                .decorated
                  span.decoration
                  h5.section__head-title Мои объявления
                  span.decoration
              .container
                .row
                  .baraholka-item.col-xl-12
                    .baraholka-item__container
                      .row
                        .baraholka-item__image.col-xl-3.col-sm-4.col-12
                          a(href="#").cover-box
                            img(data-src="img/gallery/barahlo.jpg", alt="").lazy
                        .baraholka-item__content.col-xl-9.col-sm-8.col-12
                          .row
                            .baraholka-item__info.col-xl-12
                              .row
                                .col-xl.col-sm-12
                                  a(href="#").baraholka-item__title Кресло мужское Барбер мд-8779
                                  span.baraholka-item__text Мебель | Москва, м. Отрадное
                                .col-xl-auto
                                  span.baraholka-item__price.t-size-36.f-w-b 79 990 р.
                            .baraholka-item__buttons.col-xl-12
                              .row
                                .col-xl-12
                                  .row.row_flex-end
                                    .col-xl-auto
                                      .button.button_style9.small.events-none
                                        i.far.fa-eye
                                        | 100
                                .col-xl-4.col-sm-6
                                  button.button.button_style7.small
                                    i.far.fa-eye-slash
                                    | Скрыть
                                .col-xl-4.col-sm-6
                                  button.button.button_style9.small
                                    i.far.fa-trash-alt
                                    | Удалить
                                .col-xl-4.col-sm-12
                                  button.button.button_style1.small
                                    | Редактировать
                  .baraholka-item.col-xl-12.hidden
                    .baraholka-item__container
                      .row
                        .baraholka-item__image.col-xl-3.col-sm-4.col-12
                          a(href="#").cover-box
                            img(data-src="img/gallery/barahlo.jpg", alt="").lazy
                        .baraholka-item__content.col-xl-9.col-sm-8.col-12
                          .row
                            .baraholka-item__info.col-xl-12
                              .row
                                .col-xl.col-sm-12
                                  a(href="#").baraholka-item__title Кресло мужское Барбер мд-8779
                                  span.baraholka-item__text Мебель | Москва, м. Отрадное
                                .col-xl-auto
                                  span.baraholka-item__price.t-size-36.f-w-b 79 990 р.
                            .baraholka-item__buttons.col-xl-12
                              .row
                                .col-xl-12
                                  .row.row_flex-end
                                    .col-xl-auto
                                      .button.button_style9.small.events-none
                                        i.far.fa-eye
                                        | 100
                                .col-xl-4.col-sm-6
                                  button.button.button_style7.small
                                    i.far.fa-eye
                                    | Показать
                                .col-xl-4.col-sm-6
                                  button.button.button_style9.small
                                    i.far.fa-trash-alt
                                    | Удалить
                                .col-xl-4.col-sm-12
                                  button.button.button_style1.small
                                    | Редактировать
                    
                  
        //footer
        include ../blocks/footer
        //other
        include ../blocks/popup
        include ../blocks/fixed

    script(src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js")
    script(src="js/libs.min.js")                  
    script(src="js/common.min.js")