                          a(href="#").checkbox.checkbox_type-category.tooltip
                            svg.icon
                              use(xlink:href="img/icons/baraholka-sprite.svg#ico7")    
                    .form__item.col-xl-4.col-sm-6.col-12
                      button.button.button_style4 Найти  
                .mb-30
                  .row
                    .baraholka-item.col-xl-12
                      .baraholka-item__container
                        .row
                          .col-xl-12
                            .row
                              .baraholka-item__image.col-xl-3.col-sm-4.col-12
