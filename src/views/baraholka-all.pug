                                  img(data-src="img/gallery/barahlo.jpg", alt="").lazy
                              .baraholka-item__content.col-xl-9.col-sm-8.col-12
                                .row
                                  .baraholka-item__info.col-xl-12
                                    .row
                                      .col-xl.col-sm-12
