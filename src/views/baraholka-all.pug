          main.main
            section.section
              .section__head
                .decorated
                  span.decoration
                  h5.section__head-title Барахолка
                  span.decoration
              .container
                form(action="").form.mb-30
                  .row
                    .form__group.col-xl-12
                      .row
                        .form__item.col-xl-8.col-12
