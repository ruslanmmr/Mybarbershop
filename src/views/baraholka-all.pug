                          input(type="text" placeholder="Москва").input.input_style4
                    .form__group.col-xl-12
                      span.item-label Категория:
                      .row
                        .col-xl-auto
                          a(href="#").checkbox.checkbox_type-category.tooltip
                            svg.icon
                              use(xlink:href="img/icons/baraholka-sprite.svg#ico1")
